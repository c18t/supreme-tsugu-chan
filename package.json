{
  "name": "supreme-tsugu-chan",
  "version": "0.0.0",
  "main": "index.ts",
  "repository": "ssh://git@github.com/c18t/supreme-tsugu-chan.git",
  "author": "Uchi <u@chimata.org>",
  "license": "MIT",
  "scripts": {
    "build:front": "webpack",
    "build:back": "mix compile",
    "watch:front": "webpack --watch",
    "watch:back": "mix phx.server",
    "start:server": "yarn watch",
    "start:client": "wait-on http://0.0.0.0:4000/ && opener http://0.0.0.0:4000/",
    "release:front": "webpack --optimize-minimize",
    "release:back": "mix phx.digest && mix release",
    "build": "npm-run-all -s build:*",
    "watch": "npm-run-all -p watch:*",
    "start": "npm-run-all -p start:*",
    "release": "cross-env-shell MIX_ENV=prod NODE_ENV=production \"npm-run-all -s release:*\"",
    "clean": "mix phx.digest.clean && rimraf ./priv/static/js/app.* && rimraf ./_build"
  },
  "dependencies": {
    "@types/msgpack5": "^3.4.1",
    "@types/phoenix": "^0.0.4",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "msgpack5": "^4.2.0",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "phoenix": "^1.3.0",
    "prettier": "^1.13.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^4.3.0",
    "typescript": "^2.9.1",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-property-decorator": "^6.1.0",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.5",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "opener": "^1.4.3",
    "rimraf": "^2.6.2",
    "wait-on": "^2.1.0"
  }
}
